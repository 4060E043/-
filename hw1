C語言
#include <stdio.h>
 
int main ()
{
   
   int a = 10;

   while( a < 20 )
   {
   	printf("%d\n",a);
       a++;
   }
 
   return 0;
}




C++
#include <iostream>
using namespace std;
 
int main ()
{
   
   int a = 10;

   while( a < 20 )
   {
       cout << "a 的值：" << a << endl;
       a++;
   }
 
   return 0;
}


JAVA
public class Test {
   public static void main(String args[]) {
      int x = 10;
      while( x < 20 ) {
         System.out.print("value of x : " + x );
         x++;
         System.out.print("\n");
      }
   }
}
